{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb",
        "prettier"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "allowImportExportEverywhere": false,
        "codeFrame": false,
        "ecmaFeatures": {
            "globalReturn": true,
            "impliedStrict": true,
            "jsx": true,
            "arrowFunction": true
        }
    },
    "plugins": [
        "prettier",
        "react"
    ],
    "rules": {
        "prettier/prettier": [
            "error"
        ],
        "react/destructuring-assignment": [
            0
        ],
        "react/prop-types": [
            0
        ],
        "react/jsx-one-expression-per-line": [
            0
        ]
    }
}